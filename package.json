{
  "name": "@core/data",
  "version": "0.0.14",
  "description": "GitPod for Typescript",
  "repository": "https://github.com/chlbri/core-data.git",
  "author": "chlbri <bri_lvi@icloud.com>",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "build": "pnpm run test && pnpm run tsc && pnpm run lint",
    "build:-test": "pnpm run tsc && pnpm run lint",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx,yml,json}\"",
    "lint": "pnpm run prettier && pnpm run eslint -- --fix",
    "rm": "./node_modules/.bin/rimraf -rf node_modules pnpm.lock",
    "rinit": "pnpm run rm && pnpm run pnpm",
    "publish": "pnpm run rm && pnpm run clean && pnpm run pnpm",
    "pnpm": "pnpm install",
    "upgrade": "pnpm upgrade",
    "upgrade-internal": "pnpm upgrade core core-promises core-test",
    "git-push": "git push --follow-tags",
    "git-build-push": "pnpm install && pnpm build && pnpm run git-push",
    "git-publish": "pnpm run git-build-push && git checkout main && git merge dev && pnpm run git-push && git checkout dev"
  },
  "devDependencies": {
    "@core_chlbri/test-machine": "^0.1.7",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/object.omit": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "core": "https://github.com/chlbri/core.git#publish",
    "core-promises": "https://github.com/chlbri/core-promises.git",
    "core-test": "https://github.com/chlbri/core-test.git",
    "dayjs": "^1.10.7",
    "dequal": "^2.0.2",
    "edit-json-file": "^1.6.0",
    "eslint": "^8.0.1",
    "faker": "^5.5.3",
    "immer": "^9.0.6",
    "jest": "^27.3.0",
    "jest-cucumber": "^3.0.1",
    "jest-extended": "^1.1.0",
    "mongodb": "^4.1.3",
    "nanoid": "^3.1.30",
    "object.omit": "^3.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "zod": "^3.11.6"
  },
  "peerDependencies": {
    "core": "https://github.com/chlbri/core.git#publish",
    "core-promises": "https://github.com/chlbri/core-promises.git#publish",
    "zod": "^3.10.1"
  },
  "dependencies": {
    "@core_chlbri/core": "^0.1.1-dc",
    "xstate": "^4.26.1"
  }
}
